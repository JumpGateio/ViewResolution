{
    "name": "jumpgate/view-resolution",
    "description": "A core set of features for laravel sites.",
    "license": "MIT",
    "authors": [
        {
            "name": "Stygian",
            "email": "stygian.warlock.v2@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JumpGate\\ViewResolution\\": "src/JumpGate/ViewResolution"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JumpGate\\ViewResolution\\Providers\\ViewServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
